package ua.com.foxmindedmassive;

public class Massive {

	public static void main(String[] args) {
		
		String someWord = "Bob Kim Elza";
		String [] firstMassive = someWord.split(" ");

	
	for (int i = 0 ; i < someWord.length()  ; i ++) {
		
		
	
		System.out.println(firstMassive[i]);
	}
	
		
	

	}

}
